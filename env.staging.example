# DataBot Staging Environment Configuration

# =============================================================================
# REQUIRED SETTINGS
# =============================================================================

# Discord Bot Configuration (staging bot)
DISCORD_BOT_TOKEN=your_staging_discord_bot_token_here
DISCORD_GUILD_ID=your_staging_guild_id_here

# YouTube API Configuration
YOUTUBE_API_KEY=your_youtube_api_key_here

# =============================================================================
# DATABASE CONFIGURATION
# =============================================================================

# PostgreSQL Database
POSTGRES_PASSWORD=staging_secure_password_here

# =============================================================================
# APPLICATION CONFIGURATION
# =============================================================================

# Environment
ENVIRONMENT=staging

# Logging
LOG_LEVEL=INFO

# Rate Limiting (more lenient for testing)
RATE_LIMIT_REQUESTS=20
RATE_LIMIT_WINDOW=60

# Cache Configuration
CACHE_TTL=1800

# Bot Configuration
COMMAND_PREFIX=!
MAX_VIDEOS_PER_USER=50

# =============================================================================
# MONITORING (Optional for staging)
# =============================================================================

# Grafana (optional)
# GRAFANA_ADMIN_PASSWORD=staging_grafana_password
